{
  "name": "chopsticks",
  "version": "0.0.0",
  "description": "more greedy argument parser like a minimist!",
  "main": "lib",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "abby build, tap, cover",
    "start": "mocha --watch",
    "tap-watch": "abby build, tap --watch --log",
    "tap": "cross-env NODE_ENV=minimist tap test-minimist-v1.2.0/*.js",
    "lint": "eslint src test",
    "cover": "abby cover:*",
    "cover:test": "nyc --reporter=lcov --reporter=text mocha",
    "cover:report": "node -e \"if(process.env.TRAVIS){}else{process.exit(1)}\" && codeclimate-test-reporter < coverage/lcov.info || echo skip coverage report",
    "build": "cross-env NODE_ENV=production rollup -c",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "abigail": {
    "plugins": {
      "log": false,
      "watch": false,
      "parse": "serial",
      "launch": "force"
    }
  },
  "devDependencies": {
    "abigail": "^1.3.5",
    "array-from": "59naga/array-from#9cd0f1ce9a0e0adbfd21d9b7b735bc5765ba969e",
    "assert-exception": "^1.1.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-espower": "^2.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "chokidar": "^1.4.3",
    "codeclimate-test-reporter": "^0.3.1",
    "conventional-changelog-cli": "^1.1.1",
    "cross-env": "^1.0.7",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "lodash.get": "^4.2.0",
    "lodash.set": "^4.1.0",
    "mocha": "^2.4.5",
    "nyc": "^6.1.1",
    "power-assert": "^1.3.1",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-npm": "^1.4.0",
    "rollup-plugin-uglify": "^0.3.1",
    "sinon": "^1.17.3",
    "tap": "^5.7.0",
    "tape": "^4.5.1"
  },
  "keywords": [
    "cli",
    "bin",
    "util",
    "utility",
    "helper",
    "command",
    "line",
    "parser",
    "option",
    "flags",
    "input",
    "cmd",
    "console",
    "argv",
    "getopt",
    "command",
    "option",
    "parser",
    "minimist",
    "optimist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/chopsticks.git"
  },
  "author": "59naga <i59naga@icloud.com> (http://berabou.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/59naga/chopsticks/issues"
  },
  "homepage": "https://github.com/59naga/chopsticks#readme"
}
